{
    "version": 1,
    "projects": {
        "vending-machine": {
            "root": "libs/vending-machine",
            "sourceRoot": "libs/vending-machine/src",
            "projectType": "library",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/vending-machine/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/vending-machine"],
                    "options": {
                        "jestConfig": "libs/vending-machine/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "vending-machine-e2e": {
            "root": "apps/vending-machine-e2e",
            "sourceRoot": "apps/vending-machine-e2e/src",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@nrwl/cypress:cypress",
                    "options": {
                        "cypressConfig": "apps/vending-machine-e2e/cypress.json",
                        "tsConfig": "apps/vending-machine-e2e/tsconfig.e2e.json",
                        "devServerTarget": "vending-machine:serve"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "vending-machine:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/vending-machine-e2e/**/*.{js,ts}"
                        ]
                    }
                }
            }
        },
        "ui-components": {
            "root": "libs/ui-components",
            "sourceRoot": "libs/ui-components/src",
            "projectType": "library",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/ui-components/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/ui-components"],
                    "options": {
                        "jestConfig": "libs/ui-components/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        },
        "engine": {
            "root": "libs/engine",
            "sourceRoot": "libs/engine/src",
            "projectType": "library",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": ["libs/engine/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "outputs": ["coverage/libs/engine"],
                    "options": {
                        "jestConfig": "libs/engine/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular"
    },
    "schematics": {
        "@nrwl/angular": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        },
        "@nrwl/angular:component": {
            "style": "scss"
        }
    },
    "defaultProject": "vending-machine"
}
